#ifndef OSQP_CONFIGURE_H
# define OSQP_CONFIGURE_H


/* DEBUG */
#cmakedefine DEBUG

/* Operating system */
#cmakedefine IS_LINUX
#cmakedefine IS_MAC
#cmakedefine IS_WINDOWS

/* ALGEBRA - Only one of the following is defined at compilation time */
#cmakedefine ALGEBRA_DEFAULT
#cmakedefine ALGEBRA_MKL
#cmakedefine ALGEBRA_CUDA

/* ALGEBRA - String representation */
#cmakedefine OSQP_ALGEBRA "@OSQP_ALGEBRA@"

/* OSQP_CODEGEN */
#cmakedefine OSQP_CODEGEN

/* EMBEDDED */
#cmakedefine EMBEDDED (@EMBEDDED@)

/* OSQP_ENABLE_PRINTING */
#cmakedefine OSQP_ENABLE_PRINTING

/* Header file containing custom printing functions */
#cmakedefine OSQP_CUSTOM_PRINTING "@OSQP_CUSTOM_PRINTING@"

/* PROFILING */
#cmakedefine PROFILING

/* OSQP_ENABLE_INTERRUPT */
#cmakedefine OSQP_ENABLE_INTERRUPT

/* DFLOAT */
#cmakedefine DFLOAT

/* DLONG */
#cmakedefine DLONG

#endif /* ifndef OSQP_CONFIGURE_H */
